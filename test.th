n = 2
while(n < 100) {
    prime = 1
    d = 1
    while((d = d + 1) * d <= n)
        if(n % d == 0) prime = 0
    if(prime) print(n)
    n = n + 1
}

Stack = new Object
{
    value = nil
    next = nil
    
    init = #(v) new Stack { value = v }
    push = #(v) new Stack { value = v next = _ }
    
    printl = #() {
        if(next != nil) next.printl()
        print(value)
    }
}

s = Stack.init(5)
s = s.push(7)
s.printl()